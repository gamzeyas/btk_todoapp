{
    "@@locale": "tr",

    "appName": "Todo Uygulaması",
    "@appName": {
        "description": "Lütfen uygulama adını değiştirmeyin"
    },

    "loginMessageError": "Kullanıcı adınızı veya parolanızı kontrol edin",
    "@loginMessageError": {
        "description": "Giriş hatası mesajı"
    },

    "signupMessageError": "Lütfen kullanıcı adınızı, adınızı, soyadınızı veya şifrenizi kontrol edin",
    "@signupMessageError": {
        "description": "Geçersiz veya hatalı kullanıcı adı, ad, soyad veya şifre için hata mesajı"
    },

    "loginMessageSuccess": "{email} olarak giriş yaptınız",
    "@loginMessageSuccess": {
        "description": "Girişten sonra başarı mesajı",
        "placeholders": {
            "email": {
                "type": "String"
            }
        }
    },

    "logoutMessageSuccess": "Oturumunuz kapatıldı",
    "@logoutMessageSuccess": {
        "description": "Kullanıcının çıkış yaptığında gösterilen mesaj."
    },

    "signupMessageSuccess": "{email} olarak kaydoldu",
    "@signupMessageSuccess": {
        "description": "Kayıttan sonra başarı mesajı",
        "placeholders": {
            "email": {
                "type": "String"
            }
        }
    },

    "authFormUsername": "Kullanıcı Adı",
    "@authFormUsername": {
        "description": "Kullanıcı adı alanı etiketi"
    },

    "authFormName": "Ad",
    "@authFormName": {
        "description": "Ad alanı için etiket"
    },
    
    "authFormSurname": "Soyad",
    "@authFormSurname": {
        "description": "Soyad alanı için etiket"
    },

    "authFormPassword": "Şifre",
    "@authFormPassword": {
        "description": "Şifre alanı etiketi"
    },

    "authFormErrorsFill": "Lütfen {fieldname} alanını doldurun",
    "@authFormErrorsFill": {
        "description": "Boş giriş alanı için hata mesajı",
        "placeholders": {
            "fieldname": {
                "type": "String"
            }
        }
    },

    "authFormErrorsInvalid": "Lütfen geçerli bir {fieldname} girin",
    "@authFormErrorsInvalid": {
        "description": "Geçersiz giriş için hata mesajı",
        "placeholders": {
            "fieldname": {
                "type": "String"
            }
        }
    },

    "authNoAccount": "Henüz bir hesabın yok mu?",
    "@authNoAccount": {
        "description": "Giriş sayfasında kullanıcının hesabı olup olmadığını soran mesaj"
    },
    
    "authAlreadyAccount": "Zaten bir hesabın var mı?",
    "@authAlreadyAccount": {
        "description": "Kayıt olma sayfasında kullanıcının zaten hesabı olup olmadığını soran mesaj"
    },

    "authButtonLogIn": "Giriş Yap",
    "@authButtonLogIn": {
        "description": "Giriş butonu metni"
    },

    "authButtonSignUp": "Kaydol",
    "@authButtonSignUp": {
        "description": "Kayıt olma sayfasında kayıt olma butonu metni"
    },

    "authButtonLogOut": "Çıkış Yap",
    "@authButtonLogOut": {
        "description": "Çıkış butonu metni"
    }
